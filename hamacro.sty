%% section formatting
\newcommand\sectionformat[2]{
  \tikz {
    \node [
      draw=subcolor,
      line width=1.5pt,
      fill=gray!20,
      align=left,
      rounded corners=6pt,
      text width=\textwidth
    ] {\textcolor{subcolor}{#2}\;\textcolor{black}{#1}};
  }
}

\titleformat{name=\section}
            {\sf\gtfamily\bfseries\LARGE}{}{0pt}
            {\sectionformat{#1}{\textrm\thesection}}
\titleformat{name=\section,numberless}
            {\sf\gtfamily\bfseries\LARGE}{}{0pt}
            {\sectionformat{#1}{}}
\titlespacing*{\section}{-1em}{3ex}{-4ex}

\titleformat{name=\subsection}[block]
            {\sf\gtfamily\bfseries\Large}
            {\textcolor{subcolor}{\thesubsection}}
